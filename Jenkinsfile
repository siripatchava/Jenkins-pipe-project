pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo "build code"
            }
        }
        stage('test') {
            steps {
                echo "running tests"
            }
        }
        stage('package') {
            steps {
                echo "packing code"
            }
        }
        stage('deploy') {
            steps {
                echo "deploy code"
            }
        }
    }
}